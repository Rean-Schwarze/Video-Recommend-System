}

/*LISTWIDGET*/
QListView {
    background-color: transparent;
    border: 1.5px solid rgb(0, 0, 0, 13);
    border-radius: 5px;
    outline: 0;
    padding-right: 5px;
}

QListView::item {
    padding: 7px;
    margin-top: 3px;
}

QListView::item:selected {
    color: rgb(0, 0, 0);
    background-color: rgb(0, 0, 0, 7);
    border-radius: 5px;
    margin-bottom: 3px;
    padding-left: 0px;
}

QListView::item:!selected:hover {
    background-color: rgb(0, 0, 0, 13);
    border-radius: 5px;
    margin-bottom: 3px;
    padding-left: 0px;
}

/*COMBOBOX*/
QComboBox {
    background-color: rgb(0, 0, 0, 7);
    border: 1px solid rgb(0, 0, 0, 13);
    border-radius: 5px;
    padding-left: 10px;
    min-height: 38px;
    max-height: 38px;
}

QComboBox:hover {
    background-color: rgb(0, 0, 0, 13);
    border: 1px solid rgb(0, 0, 0, 13);
}

QComboBox::pressed {
    border: 1px solid rgb(0, 0, 0, 10);
}

QComboBox::down-arrow {
    image: url(:/icon/ComboBox.png);
}

QComboBox::drop-down {
    background-color: transparent;
    min-width: 50px;
}

QComboBox:disabled {
    color: rgb(0, 0, 0, 110);
    background-color: rgb(0, 0, 0, 13);
    border: 1px solid rgb(0, 0, 0, 5);
}

QComboBox::down-arrow:disabled {
    image: url(:/icon/ComboBoxDisabled.png);
}</string>
